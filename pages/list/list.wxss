.main {
  display: flex;
}
.main .left {
  width: 100px;
  color: white;
  height: 100vh;
  overflow: scroll;
}
.main .right {
  width: 100%;
  height: 100vh;
  overflow: scroll;
}
.main .right .title {
  text-align: center;
  font-size: 14px;
  margin: 20px 0;
}
.main .right .img_box {
  display: flex;
  flex-wrap: wrap;
  width: 100%;
}
.main .right .img_box .image_box {
  width: 30%;
  height: 50px;
  margin: 3px;
}
.main .right .img_box .image_box .img {
  width: 100%;
  height: 70%;
  text-align: center;
}
.main .right .img_box .image_box .img image {
  width: 50%;
  height: 100%;
}
.main .right .img_box .image_box .title {
  font-size: 12px;
  margin: 4px;
}
